{
  "name": "mucan54/tauri-php",
  "description": "Build cross-platform desktop applications with Laravel and Tauri. Embed your Laravel app into native desktop apps for Windows, macOS, and Linux using FrankenPHP.",
  "keywords": [
    "laravel",
    "tauri",
    "desktop",
    "cross-platform",
    "frankenphp",
    "electron-alternative",
    "native-apps",
    "php-desktop",
    "desktop-applications"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mucan",
      "email": "mucan@example.com"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/console": "^10.0|^11.0",
    "symfony/process": "^6.0|^7.0",
    "symfony/filesystem": "^6.0|^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0",
    "laravel/pint": "^1.0",
    "mockery/mockery": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Mucan54\\TauriPhp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mucan54\\TauriPhp\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mucan54\\TauriPhp\\TauriPhpServiceProvider"
      ],
      "aliases": {
        "TauriPhp": "Mucan54\\TauriPhp\\Facades\\TauriPhp"
      }
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "format": "vendor/bin/pint"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
